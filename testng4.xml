<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="Factile_Mode_v3.Listeners2" />
	</listeners>

      <test name="Double Factile">
    <classes>
          <class name="Factile_Mode_v3.DoubleFactile"/>
           </classes>
  </test> <!-- Test -->
   

	<test name="Display Answer Button">
		<classes>
			<class name="Factile_Mode_v3.DisplayAnswerButton" />
		</classes>
	</test> <!-- Test -->

	<test name="Display Exact Case When Checked">
		<classes>
			<class name="Factile_Mode_v3.DisplayExactCaseChecked" />
		</classes>
	</test> <!-- Test -->

	<test name="Display Exact Case When UNChecked">
		<classes>
			<class name="Factile_Mode_v3.DisplayExactCaseUnChecked" />
		</classes>
	</test> <!-- Test -->
	<test name="Display Question On GameBoard >> When Checked">
		<classes>
			<class
				name="Factile_Mode_v3.DisplayQuestionOnGameBoardTileChecked" />
		</classes>
	</test> <!-- Test -->
	<test name="Display Question On GameBoard >> When UNChecked">
		<classes>
			<class
				name="Factile_Mode_v3.DisplayQuestionOnGameBoardTileUnChecked" />
		</classes>
	</test> <!-- Test -->

	<test name="Hide Category With Points >> When Checked">
		<classes>
			<class name="Factile_Mode_v3.HideCategoryWithPointsChecked" />
		</classes>
	</test> <!-- Test -->

	<test name="Hide Category With Points When UnChecked">
		<classes>
			<class name="Factile_Mode_v3.HideCategoryWithPointsUnChecked" />
		</classes>
	</test> <!-- Test -->

<test name="Play as Jeopardy Style instead of Number">
		<classes>
			<class name="Factile_Mode_v3.PlayAsJeopardyStyle" />
		</classes>
	</test> <!-- Test -->
	
	<test name="Play as Numbered tiles instead of Jeopardy Style">
		<classes>
			<class name="Factile_Mode_v3.PlayAsNumberedTilesInsteadOfJeopardyStyleChecked" />
		</classes>
	</test> <!-- Test -->

   
      <test name="Daily Double">
    <classes>
          <class name="Factile_Mode_v3.DailyDouble"/>
           </classes>
  </test> <!-- Test -->
   
     <test name="Manually EnableAndDisable The Buzzer">
    <classes>
          <class name="Factile_Mode_v3.ManuallyEnableAndDisableTheBuzzer"/>
           </classes>
  </test> <!-- Test -->
   

   
   
    <test name="Show Skip Question Button">
    <classes>
          <class name="Factile_Mode_v3.ShowSkipQuestionButton"/>
           </classes>
  </test> <!-- Test -->
  
    <test name="Skip And See Answer">
    <classes>
          <class name="Factile_Mode_v3.SkipAndSeeAnswer"/>
           </classes>
  </test> <!-- Test -->
   


</suite> <!-- Suite -->
